<script type="text/javascript">
function pathSpeakerShow(id) {
	return String("{{ path('speakersShow', {'id': '_id_'}) }}").replace("_id_", id);
}

function pathSoundsShow(id) {
	return String("{{ path('soundsShow', {'id': '_id_'}) }}").replace("_id_", id);
}

String.prototype.open = function(target) {
	if (target) {
		window.open(this, target);
	}
	else {
		window.location.href = this;
	}
};

</script>
